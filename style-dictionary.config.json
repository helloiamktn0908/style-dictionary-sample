{
  "source": ["tokens/style-dictionary-tokens.json"],
  "platforms": {
    "ts": {
      "transformGroup": "js",
      "files": [
        {
          "format": "javascript/es6",
          "destination": "build/colors.js"
        },
        {
          "format": "typescript/es6-declarations",
          "destination": "build/colors.d.ts"
        }
      ]
    }
  }
}

{
  "source": ["tokens/style-dictionary-tokens.json"],
  "platforms": {
    "js": {
      "transformGroup": "js",
      "buildPath": "build/",
      "files": [
        {
          "format": "javascript/es6",
          "destination": "design-tokens.ts"
        },
        {
          "format": "typescript/es6-declarations",
          "destination": "design-tokens.d.ts"
        }
      ]
    }
  }
}
